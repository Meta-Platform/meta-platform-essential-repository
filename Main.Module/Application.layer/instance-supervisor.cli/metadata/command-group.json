                {
                    "key":"socket",
                    "paramType":"positional",
                    "valueType":"string",
                    "describe":"Nome do arquivo de sockets"
                }
            ],
            "parametersToLoad": [
                "supervisorLib",
                "supervisorSocketsDirPath"
            ]
        },
        {
            "commandName": "Show",
            "command" : "show",
            "children": [
                    {
                        "commandName": "ShowExecutionTaskInformation",
